<%- include('../partials/header') %>

<div class="post-cards">
    <% for (let cluck of clucks) { %>
        <div class="card mb-4">
            <img class="small-image" src="<%= cluck.image_url %>" alt="Card image cap">
            <div class="card-body">
                <h5 class="card-title"><%= loggedInUserName %><%= cluck.created_at %></h5>
                <p class="card-text"><%= cluck.content %></p>
            </div>
        </div>
    <% } %>

<%- include('../partials/footer') %>